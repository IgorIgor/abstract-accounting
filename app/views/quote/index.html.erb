<table>
  <thead>
    <tr>
      <th><%=t 'views.quote.resource' %></th>
      <th><%=t 'views.quote.date' %></th>
      <th><%=t 'views.quote.rate' %></th>
    </tr>
  </thead>
  <tbody data-bind="foreach: documents">
    <tr>
      <td data-bind="text: resource"></td>
      <td data-bind="text: day"></td>
      <td data-bind="text: rate"></td>
    </tr>
  </tbody>
</table>
