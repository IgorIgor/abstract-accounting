<table>
  <thead>
    <tr>
      <th id="tag" data-bind="click: sortBy">
        <%= t 'views.entities.tag' %><span></span>
      </th>
      <th id="type" data-bind="click: sortBy">
        <%= t 'views.entities.type' %><span></span>
      </th>
    </tr>
  </thead>
  <tbody data-bind="foreach: documents">
    <tr>
      <td data-bind="text: tag, click: $parent.showBalances"></td>
      <td data-bind="text: type, click: $parent.showBalances"></td>
    </tr>
  </tbody>
</table>
