<div id="<%= dialog_id %>" class="dialog" data-bind="with: <%= dialog_model %>">
  <div class="table-mode">
    <% if local_assigns.has_key? :custom_search %>
      <%= render partial: custom_search %>
    <% else %>
      <input type="text" id='dialog_search' data-bind="value: filter.tag"/>
    <% end %>
    <input type="button" value="<%= t('views.home.search') %>"
           data-bind="click: filterData"/>
  </div>
  <%= render partial: "home/paginate" %>
  <%= render partial: data_path %>
</div>
