<table>
  <thead>
    <tr>
      <th><%=t 'views.users.user_name' %></th>
      <th><%=t 'views.users.email' %></th>
    </tr>
  </thead>
  <tbody data-bind="foreach: documents">
    <tr>
      <td data-bind="text: tag, click: $parent.showUser"></td>
      <td data-bind="text: email, click: $parent.showUser"></td>
    </tr>
  </tbody>
</table>
