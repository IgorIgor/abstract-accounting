<table>
  <thead>
    <tr>
      <th><%= t 'views.user_notification.title' %></th>
      <th><%= t 'views.user_notification.date' %></th>
    </tr>
  </thead>
  <tbody data-bind="foreach: documents">
    <tr>
      <td data-bind="text: title, click: $parent.show"></td>
      <td data-bind="text: date, click: $parent.show"></td>
    </tr>
  </tbody>
</table>
