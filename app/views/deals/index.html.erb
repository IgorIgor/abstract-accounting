<table>
  <thead>
    <tr>
      <th><%= t 'views.deals.tag' %></th>
      <th><%= t 'views.deals.entity' %></th>
      <th><%= t 'views.deals.give' %></th>
      <th><%= t 'views.deals.take' %></th>
      <th><%= t 'views.deals.rate' %></th>
    </tr>
  </thead>
  <tbody data-bind="foreach: documents">
    <tr>
      <td data-bind="text: tag"></td>
      <td data-bind="text: entity_tag"></td>
      <td data-bind="text: give"></td>
      <td data-bind="text: take"></td>
      <td data-bind="text: rate"></td>
    </tr>
  </tbody>
</table>
