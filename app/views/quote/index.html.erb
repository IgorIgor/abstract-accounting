<table>
  <thead>
    <tr>
      <th id="alpha_code" data-bind="click: sortBy">
        <%=t 'views.quote.resource' %><span></span>
      </th>
      <th id="day" data-bind="click: sortBy">
        <%=t 'views.quote.date' %><span></span>
      </th>
      <th id="rate" data-bind="click: sortBy">
        <%=t 'views.quote.rate' %><span></span>
      </th>
    </tr>
  </thead>
  <tbody data-bind="foreach: documents">
    <tr>
      <td data-bind="text: resource, click: $parent.show"></td>
      <td data-bind="text: day, click: $parent.show"></td>
      <td data-bind="text: rate, click: $parent.show"></td>
    </tr>
  </tbody>
</table>
